{
    "name": "slam/mysql-php",
    "description": "PHP version of mysql cli that comes with MySQL",
    "license": "MIT",
    "keywords": [
        "mysql"
    ],
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com"
        }
    ],
    "require": {
        "php": "~8.0.0 || ~8.1.0",
        "ext-mysqli": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8.0",
        "phpstan/phpstan": "^1.5.7",
        "phpstan/phpstan-phpunit": "^1.1.1",
        "phpstan/phpstan-strict-rules": "^1.1.0",
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload": {
        "psr-4": {
            "SlamMysql\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamMysql\\Tests\\": "tests/"
        }
    },
    "bin": [
        "mysql"
    ]
}
