{
    "name": "slam/mysql-php",
    "description": "PHP version of mysql cli that comes with MySQL",
    "license": "MIT",
    "keywords": [
        "mysql"
    ],
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "ext-mysqli": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14.4",
        "phpstan/phpstan": "^1.10.1",
        "phpstan/phpstan-phpunit": "^1.3.7",
        "phpstan/phpstan-strict-rules": "^1.5.0",
        "phpunit/phpunit": "^10.0.11"
    },
    "autoload": {
        "psr-4": {
            "SlamMysql\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamMysql\\Tests\\": "tests/"
        }
    },
    "bin": [
        "mysql"
    ]
}
